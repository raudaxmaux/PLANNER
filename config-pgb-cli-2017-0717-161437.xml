<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget android-versionCode="2" id="br.com.applanner" version="0.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>APPlanner</name>
  <description>Aplicativo de controle e planejamento para profissionais de festas e eventos</description>
  <author>Brito Hiperm√≠dia</author>
  <content src="index.html"/>
  <preference name="android-build-tool" value="gradle"/>
  <preference name="phonegap-version" value="cli-6.2.0"/>
  <preference name="xwalkMultipleApk" value="false"/>
  <preference name="SplashScreen" value="none"/>
  <preference name="SplashScreenDelay" value="0"/>
  <preference name="FadeSplashScreenDuration" value="0"/>
  <access origin="*"/>
  <access origin="tel:*" launch-external="yes"/>
  <plugin name="cordova-plugin-device" spec="1.1.2"/>
  <plugin name="cordova-plugin-compat" spec="1.0.0"/>
  <plugin name="cordova-plugin-console" spec="1.0.6"/>
  <plugin name="cordova-plugin-contacts" spec="2.2.0">
    <variable name="CONTACTS_USAGE_DESCRIPTION" value=" "/>
  </plugin>
  <plugin name="cordova-plugin-dialogs" spec="1.2.1"/>
  <plugin name="cordova-plugin-file" spec="4.2.0"/>
  <plugin name="cordova-plugin-file-transfer" spec="1.5.1"/>
  <plugin name="cordova-plugin-inappbrowser" spec="1.4.0"/>
  <plugin name="cordova-plugin-media" spec="2.3.0"/>
  <plugin name="cordova-plugin-network-information" spec="1.2.1"/>
  <plugin name="cordova-plugin-statusbar" spec="2.1.3"/>
  <plugin name="cordova-plugin-whitelist" spec="1.3.2"/>
  <plugin name="ionic-plugin-keyboard" spec="2.2.1"/>
  <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
    <variable name="APP_ID" value="237799736695169"/>
    <variable name="APP_NAME" value="APPlanner"/>
  </plugin>
  <platform name="android">
    <preference name="android-minSdkVersion" value="19"/>
    <preference name="android-targetSdkVersion" value="25"/>
    <preference name="android-installLocation" value="auto"/>
    <preference name="Orientation" value="portrait"/>
    <preference name="AndroidLaunchMode" value="singleTop"/>
    <preference name="SplashScreenDelay" value="0"/>
  </platform>
  <icon density="ldpi" height="36" platform="android" src="package-assets/icone-36.png" width="36"/>
  <icon density="mdpi" height="48" platform="android" src="package-assets/icone-48.png" width="48"/>
  <icon density="hdpi" height="72" platform="android" src="package-assets/icone-72.png" width="72"/>
  <icon density="xhdpi" height="96" platform="android" src="package-assets/icone-96.png" width="96"/>
  <config-file mode="merge" parent="/manifest/application" platform="android">
    <activity android:launchMode="singleTop"/>
  </config-file>
  <allow-intent href="tel:*"/>
  <allow-navigation href="http://169.254.219.222:8100"/>
  <allow-navigation href="http://169.254.19.154:8100"/>
</widget>